{
  "mcpServers": {
    "azure-mcp": {
      "command": "npx",
      "args": [
        "@azure/mcp",
        "server",
        "start"
      ]
    },
    "deepcontext": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@wildcard-ai/deepcontext@latest"
      ],
      "env": {
        "WILDCARD_API_KEY": "${DEEPCONTEXT_API_KEY}"
      }
    }
  }
}
