# Environment variables template
# Copy this file to .env.development for local development

# Backend API URL (required)
# For local development, use: http://localhost:8000
# For production, use your Azure Container Apps URL
VITE_API_BASE_URL=http://localhost:8000

# Debug mode (optional)
# Set to true for detailed error messages and logging
VITE_DEBUG=true

# Azure AD Configuration (for Phase 5 authentication)
# VITE_AZURE_AD_CLIENT_ID=your-client-id
# VITE_AZURE_AD_TENANT_ID=your-tenant-id
# VITE_REDIRECT_URI=http://localhost:5173/auth/callback
